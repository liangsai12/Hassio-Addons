{
  "name": "Raw WireGuard",
  "version": "0.1.2",
  "slug": "rawwireguard",
  "description": "Fast, modern, secure VPN tunnel",
  "url": "https://hub.fastgit.org/zhujisheng/hassio-addons/tree/master/raw_wireguard",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "init": false,
  "privileged": [
    "NET_ADMIN"
  ],
  "devices": [
    "/dev/net/tun"
  ],
  "host_network": true,
  "startup": "once",
  "boot": "manual",
  "options": {
    "interface": {
      "PrivateKey": "youraddonsprivatekey",
      "Address": "172.27.66.8/24"
    },
    "peers": [
      {
        "PublicKey": "youvpnserverspublickey",
        "EndPoint": "your_vpn_server:51820",
        "AllowedIPs": "0.0.0.0/0"
      }
    ]
  },
  "schema": {
    "interface_name": "str?",
    "interface": {
      "PrivateKey": "password",
      "ListenPort": "port?",
      "FwMark": "str?",
      "Address": "str?",
      "DNS": "str?",
      "MTU": "int?",
      "Table": "str?",
      "PreUp": "str?",
      "PostUp": "str?",
      "PreDown": "str?",
      "PostDown": "str?",
      "SaveConfig": "bool?"
    },
    "peers": [
      {
        "PublicKey": "str",
        "PreSharedKey": "str?",
        "AllowedIPs": "str?",
        "EndPoint": "str?",
        "PersistentKeepalive": "int?"
      }
    ]
  },
  "image": "zhujisheng/{arch}-addon-rawwireguard"
}